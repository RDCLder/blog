    /* Set the width of the side navigation to 250px */
    function openNav() {
        document.getElementById("mySidenav").style.width = "100vw";
        document.getElementById("mySidenav").style.display = "block";
        document.getElementById("box1").classList.remove("fadeOutDown");
        document.getElementById("box2").classList.remove("fadeOutDown");
        document.getElementById("box3").classList.remove("fadeOutDown");
        document.getElementById("box4").classList.remove("fadeOutDown");

        setTimeout( ()=>{
            document.getElementById("box1").classList.remove("hideandflex");
            document.getElementById("box1").classList.add("d-flex");
            document.getElementById("box1").classList.add("animated");
            document.getElementById("box1").classList.add("fadeInUp");
            // document.getElementById("sidenavbox").classList.add("slow");
        },500)
        setTimeout( ()=>{
            document.getElementById("box2").classList.remove("hideandflex");
            document.getElementById("box2").classList.add("d-flex");
            document.getElementById("box2").classList.add("animated");
            document.getElementById("box2").classList.add("fadeInUp");
        },700)
        setTimeout( ()=>{
            document.getElementById("box3").classList.remove("hideandflex");
            document.getElementById("box3").classList.add("d-flex");
            document.getElementById("box3").classList.add("animated");
            document.getElementById("box3").classList.add("fadeInUp");
        },900)
        setTimeout( ()=>{
            document.getElementById("box4").classList.remove("hideandflex");
            document.getElementById("box4").classList.add("d-flex");
            document.getElementById("box4").classList.add("animated");
            document.getElementById("box4").classList.add("fadeInUp");
        },1100)
    }

    /* Set the width of the side navigation to 0 */
    function closeNav() {
        // document.getElementById("mySidenav").style.display = "none";
        setTimeout(() => {
            document.getElementById("box1").classList.remove("fadeInUp");
            document.getElementById("box1").classList.add("fadeOutDown");
        }, 100);
        setTimeout(() => {
            document.getElementById("box2").classList.remove("fadeInUp");
            document.getElementById("box2").classList.add("fadeOutDown");
        }, 200);
        setTimeout(() => {
            document.getElementById("box3").classList.remove("fadeInUp");
            document.getElementById("box3").classList.add("fadeOutDown");
        }, 300);
        setTimeout(() => {
            document.getElementById("box4").classList.remove("fadeInUp");
            document.getElementById("box4").classList.add("fadeOutDown");
        }, 400);

        setTimeout(() => {
            document.getElementById("box1").classList.remove("d-flex");
            document.getElementById("box1").classList.remove("animated");
            document.getElementById("box2").classList.remove("d-flex");
            document.getElementById("box2").classList.remove("animated");
            document.getElementById("box3").classList.remove("d-flex");
            document.getElementById("box3").classList.remove("animated");
            document.getElementById("box4").classList.remove("d-flex");
            document.getElementById("box4").classList.remove("animated");

        }, 1000);


        // document.getElementById("sidenavbox").classList.remove("slow");
        setTimeout(() => {
            document.getElementById("mySidenav").style.width = "0";
            document.getElementById("box1").classList.add("hideandflex");
            document.getElementById("box2").classList.add("hideandflex");
            document.getElementById("box3").classList.add("hideandflex");
            document.getElementById("box4").classList.add("hideandflex");
        },1000);
    }
</script>